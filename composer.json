{
    "name": "detailnet/dfw-apigility-module",
    "description": "Zend Framework Module containing tools for ZF Apigility",
    "type": "library",
    "keywords": [
        "dfw",
        "detailnet",
        "apigility",
        "module",
        "zf"
    ],
    "homepage": "https://github.com/detailnet/dfw-normalization-module/",
    "license": "MIT",
    "authors": [
        {
            "name": "Ivan Wolf",
            "email": "ivan.wolf@detailnet.ch"
        }
    ],
    "require": {
        "php": "^7.0",
        "zendframework/zend-loader": "^2.5.1",
        "zendframework/zend-modulemanager": "^2.7.2",
        "zendframework/zend-mvc": "^3.0.1",
        "zendframework/zend-paginator": "^2.7",
        "zendframework/zend-servicemanager": "^3.1",
        "zendframework/zend-stdlib": "^3.0.1",
        "zendframework/zend-view": "^2.8",
        "zfcampus/zf-apigility": "^1.3",
        "jms/serializer": "^1.5",
        "doctrine/collections": "^1.2.0",
        "detailnet/dfw-core": "^0.3",
        "detailnet/dfw-normalization": "^1.0",
        "detailnet/dfw-commanding": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.0",
        "squizlabs/php_codesniffer" : "^2.2",
        "phpmd/phpmd": "^2.2",
        "satooshi/php-coveralls": "^1.0"
    },
    "autoload": {
        "psr-0": {
            "Detail\\Apigility\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-0": {
            "DetailTest\\Apigility\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.x-dev"
        }
    },
    "archive": {
        "exclude": [
            "!vendor",
            "tests",
            "*phpunit.xml",
            ".travis.yml",
            "build.xml",
            "build.properties",
            "composer.phar"
        ]
    }
}
